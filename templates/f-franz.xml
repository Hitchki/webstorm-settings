<templateSet group="f-franz">
  <template name="co" value="console.log('$expression$', $expression$);   // *remove before push&#10;$END$" shortcut="SPACE" description="console.log" toReformat="false" toShortenFQNames="true">
    <variable name="expression" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="co2" value="if (this.debugLevel &gt;= 2) {&#10;    console.log('$expression$', $expression$);   // *remove before push&#10;}&#10;$END$" shortcut="SPACE" description="" toReformat="false" toShortenFQNames="true">
    <variable name="expression" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="ng-templ" value="&lt;ng-template $temp$ let-$par$=&quot;$par$&quot;&gt;&lt;/ng-template&gt;" description="ng-template" toReformat="false" toShortenFQNames="true">
    <variable name="temp" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="par" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="con-templ" value="&lt;ng-container *ngTemplateOutlet=&quot;$templOut$; context:{$param$: $param$}&quot;&gt;&lt;/ng-container&gt;" description="ngtemplateoutlet" toReformat="false" toShortenFQNames="true">
    <variable name="templOut" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="param" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="ddd" value="$expression$:{{$expression$ | json}}$END$" description="ddd" toReformat="false" toShortenFQNames="true">
    <variable name="expression" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="dd" value="$expression$:{{$expression$}}:$END$" description="dd" toReformat="false" toShortenFQNames="true">
    <variable name="expression" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
</templateSet>